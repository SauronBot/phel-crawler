{
    "name": "sauronbot/phel-crawler",
    "description": "Web crawler CLI written in Phel (Lisp on PHP) using Guzzle and Symfony DomCrawler",
    "keywords": [
        "phel",
        "crawler",
        "web-crawler",
        "lisp",
        "php",
        "cli"
    ],
    "homepage": "https://github.com/SauronBot/phel-crawler",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "phel-lang/phel-lang": ">=0.24",
        "guzzlehttp/guzzle": "^7.10",
        "symfony/dom-crawler": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/console": "^7.0",
        "symfony/string": "^7.0",
        "smeghead/phel-getopt": "^0.0.6"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "PhelGenerated\\": "src/PhelGenerated"
        }
    },
    "scripts": {
        "dev": "vendor/bin/phel run src/main.phel",
        "build": "vendor/bin/phel build --no-cache",
        "format": "vendor/bin/phel format",
        "test": "vendor/bin/phel test",
        "repl": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phel repl"
        ]
    }
}
